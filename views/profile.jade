doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    //
       SEO 
    title Olapic Buyers Club
    meta(name='description', content='Olapic Buyers Club')
    meta(name='keywords', content='Visual Commerce, Olapic, Buyers, Wishlist, Buying')
    link(type='text/plain', rel='author', href='humans.txt')
    link(href='favicon.ico', rel='icon', type='image/ico')
    link(href='http://fonts.googleapis.com/css?family=Lato:100,300,700|Bitter:400,700', rel='stylesheet', type='text/css')
    //
       Bootstrap 3.1.0 
    link(href='css/bootstrap.min.css', rel='stylesheet')
    //
       Flexslider 2 
    link(href='css/flexslider.css', rel='stylesheet')
    //
       ionicons font 
    //
       <link href="css/ionicons.min.css" rel="stylesheet" /> 
    //
       Font awesome font 
    link(rel='stylesheet', href='css/font-awesome/css/font-awesome.min.css')
    //
       Custom CSS 
    link(href='css/style.css', rel='stylesheet')
    link(href='css/color/blue.css', rel='stylesheet')
  body
    header
      .container
        .row
          #logo-region.col-md-3.text-center-sm
            a(href='index.html')
              img.img-responsive(src='img/logo.png', alt='logo')
          //
             /logo-region 
          #menu-region.col-md-9
            nav.navbar.nestor-main-menu(role='navigation')
              //
                 Menu button for mobile display 
              .navbar-header
                button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-ex1-collapse') MENU
              //
                 Navigation links 
              .collapse.navbar-collapse.navbar-ex1-collapse
                ul.nav.navbar-right.navbar-nav
                  li
                    a(href='/') Home
                  li
                    a(href='profile') Profile
                  li
                    a(href='about') About
                  //
                     <li><a href="contact.html">Contact</a></li> 
              //
                 /navbar-collapse 
          //
             /menu-region 
        //
           /row 
      //
         container 
    #top-content-region.bg-color-grayLight1
      .container
        .row
          #top-content-left-region.col-xs-12.col-md-6.text-center-sm
            .region
              #page-title-block.page-title.block
                h1
                  i.fa.fa-instagram.text-color-theme
                  | User Name
              //
                 /page-title-block 
            //
               /region 
          //
             /top-content-left-region 
        //
           /row 
      //
         /container 
    //
       /top-content-region 
    #content-region.region
      #portfolio-content-block.portfolio-content.block
        .container
          #grid.row
          each val, index in products
              .portfolio-item.mix.col-xs-12.col-sm-6.col-md-4
                a(href= val.url, target='_blank')
                  .portfolio-image
                    img.img-responsive(src='img/portfolio/pix1-th.jpg', alt='Portfolio 1', title='Portfolio 1')
                    .portfolio
                      h6.portfolio-title Product Name #{index} #{val.image}
                //
                   /portfolio-image 
            //
               /portfolio-item 
          //
             /row 
        //
           /container 
      //
         /portfolio-content-block 
    //
       /content-region 
    footer.bg-color-grayDark1.text-color-light
      .container
        .row
          #footer-left-region.col-xs-12.col-md-6.text-center-sm
            .region
              #copyright-block.block
                p
                  | OBC is developed by
                  a(href='http://www.olapic.com') Olapic
              //
                 /copyright-block 
            //
               /region 
          //
             /footer-left-region 
          #footer-right-region.col-xs-12.col-md-6.text-right.text-center-sm
            .region
              #social-networks-footer-block.social-networks-footer.block
                a(href='http://instagram.com/olapic')
                  i.fa.fa-instagram
                a(href='http://facebook.com/olapic')
                  i.fa.fa-facebook
                a(href='http://twitter.com/olapic')
                  i.fa.fa-twitter
                a(href='http://photorank-best.tumblr.com/')
                  i.fa.fa-tumblr
                //
                        <a href="http://linkedin.com"><i class="icon ion-social-linkedin"></i></a>
                                  <a href="http://pinterest.com"><i class="icon ion-social-pinterest"></i></a>
                                  <a href="http://vimeo.com"><i class="icon ion-social-vimeo"></i></a>
                                  <a href="http://dribbble.com"><i class="icon ion-social-dribbble-outline"></i></a>
                                  <a href="http://github.com"><i class="icon ion-social-github"></i></a> 
              //
                 /social-networks-footer 
            //
               /region 
          //
             /footer-right-region 
        //
           /row 
      //
         /container 
    //
       jQuery 1.10.2 
    script(src='js/jquery-1.10.2.min.js')
    //
       jQuery Plugins 
    script(src='js/bootstrap.min.js')
    script(src='js/jquery.flexverticalcenter.js')
    script(src='js/jquery.flexslider-min.js')
    script(src='js/retina-1.1.0.min.js')
    script(src='js/jquery.stellar.min.js')
    script(src='js/jquery.mixitup.min.js')
    script(src='http://maps.google.com/maps/api/js?sensor=false')
    script(src='js/app.js')
